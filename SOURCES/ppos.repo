[ppos-repo]
name=AP Sensing
baseurl=https://rocky1.bbn.apsensing.com/ppos-repo/$releasever/$basearch
enabled=1
skip_if_unavailable=False
metadata_expire=7d
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ppos-38-primary

[ppos-noarch-repo]
name=AP Sensing NoArch
baseurl=https://rocky1.bbn.apsensing.com/ppos-repo/$releasever/noarch
enabled=1
skip_if_unavailable=False
metadata_expire=7d
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ppos-38-primary
