[ppos-repo]
name=AP Sensing
baseurl=https://update.ppos.apsensing.com/rpm/$releasever/$basearch
enabled=1
skip_if_unavailable=False
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ppos-38-primary

[ppos-noarch-repo]
name=AP Sensing NoArch
baseurl=https://update.ppos.apsensing.com/rpm/$releasever/noarch
enabled=1
skip_if_unavailable=False
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ppos-38-primary
